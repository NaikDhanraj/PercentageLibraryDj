<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:id="@android:id/background">
        <shape>
            <corners
                android:radius="8dp" />
            <stroke
                android:width="0dip"
                android:color="@color/progress_back" />

            <solid
                android:color="@color/progress_back"/>

        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners
                    android:radius="8dp" />
                <stroke
                    android:width="0dip"
                    android:color="@color/progress_green" />
                <solid
                    android:color="@color/progress_green"/>
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:radius="8dp" />
                <stroke
                    android:width="0dip"
                    android:color="@color/progress_green" />
                <solid
                    android:color="@color/progress_green"/>

            </shape>
        </clip>
    </item>

</layer-list>