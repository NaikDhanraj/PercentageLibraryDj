<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        >
        <shape
            android:shape="rectangle"
            >
            <corners
                android:radius="8dp"
                />
            <padding
                android:right="4dp"
                android:bottom="4dp"
                android:left="4dp"
                android:top="4dp"/>
            <stroke
                android:color="@color/black"
                android:width="1dp"/>
            <solid
                android:color="@color/colorPrimary"
                />
        </shape>
    </item>
</selector>